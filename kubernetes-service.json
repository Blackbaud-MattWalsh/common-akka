{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "app": "akkademo"
    },
    "name": "akkademo"
  },
  "spec": {
    "type": "NodePort",
    "ports": [
      {
        "name": "akka-remote",
        "port": 2551,
        "protocol": "TCP",
        "targetPort": 2551
      },
      {
        "name": "tomcat",
        "port": 9001,
        "protocol": "TCP",
        "targetPort": 9001
      }
    ],
    "selector": {
      "app": "akkademo"
    }
  }
}
